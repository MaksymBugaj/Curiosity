<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pl.c.curiosity">

    <application
        android:name=".CuriosityApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_curiosity_pytajnik__1_"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_curiosity_pytajnik__1_"
        android:supportsRtl="true"
        android:theme="@style/Theme.Curiosity">
        <activity android:name=".ui.NavHolder">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>